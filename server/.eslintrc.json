{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "always"
            }
        ],
        "import/prefer-default-export": "off"
    }
}